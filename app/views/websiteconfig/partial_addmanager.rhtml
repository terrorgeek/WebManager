<h1>添加管理员</h1>
<fieldset>
<% form_tag :action=>"dispose_partial_addmanager" do %>
用户名：<%=text_field_tag "username"%><br></br>
密  码：<%=text_field_tag "password"%><br></br>
真实姓名：<%=text_field_tag "realname"%><br></br>
管理员类型：<%=select_tag "managertype","<option>ordinary</option><option>admin</option>"%><br></br>
<%=submit_tag "submit"%><br></br>
<%end%>
</fieldset>
<br></br>
<%=link_to "返回",:action=>"managermanagement"%>
