<%=stylesheet_link_tag "display"%>
      <div id="merchant1" style="margin-left: <%=@allcss.margin_left%>px;margin-top:<%=@allcss.margin_top%>px"><center><span>招商</span></center><br/><% unless @merchant.nil? %>
      <% @merchant.each do |p| %>
        <% if @allcss.picture=="yes" %>
        <img src="<%=url_for :controller=>"merchant",:action=>"picture",:id=>p.id%>" style="height: 50px;width: 50px" /><br/>
        <%end%>
        <%=p.title%><br/>
        <%=p.merchantdetails[0...30]+"....."%><br/>
      <%end%>
        <% if @allcss.draggable=="yes"%>
        <%=javascript_include_tag :defaults%>
            <%=draggable_element("merchant1")%>
        <%end%>
        <%end%>
      </div>