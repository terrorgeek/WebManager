<%=stylesheet_link_tag "display"%>
<div id="show1" style="margin-left: <%=@allcss.margin_left%>px;margin-top:<%=@allcss.margin_top%>px"><center><span>行业展会</span></center><br/><% unless @show.nil? %>
      <% @show.each do |p| %>
  <% if @allcss.picture=="yes" %>
  <img src="<%=url_for :controller=>"show",:action=>"picture",:id=>p.id%>" style="height: 50px;width: 50px" />
  <%end%>
        <%=p.showtitle%>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%=p.showcity%><br/>
      <%end%>
        <% if @allcss.draggable=="yes"%>
        <%=javascript_include_tag :defaults%>
            <%=draggable_element("show1")%>
        <%end%>
        <%end%>
</div>